   if ( dbfCodebar )->( dbSeek( cCodArt ) )

      while ( dbfCodebar )->cCodArt == cCodArt .AND. !( dbfCodebar )->( eof() )

         dbPass( dbfCodebar, dbfTmpCodebar, .T. )

