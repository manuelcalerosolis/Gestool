UTILITY STATIC function TTotalTurno_addImporte( cCaja, cSerie, nImporte, aImporte, cData, aData, bEdit, nBmp) ; local Self AS CLASS TTotalTurno := QSelf() AS CLASS TTotalTurno

   local n

   IIF( nBmp == nil, nBmp := 0, ) ;

   if ( n := aScan( aImporte, {|a| a[ 1 ] == cCaja .AND. a[ 2 ] == cSerie } ) ) <> 0
      aImporte[ n, 3 ]  += nImporte
   else
      aAdd( aImporte, { cCaja, cSerie, nImporte } )
   end

   if !Empty( cData )
      aAdd( aData, { cCaja, cSerie, cData, nImporte, bEdit, nBmp } )
   end

RETURN ( Self )



UTILITY STATIC function TTotalTurno_addNumero( cCaja, aData) ; local Self AS CLASS TTotalTurno := QSelf() AS CLASS TTotalTurno

   local n

   if ( n := aScan( aData, {|a| a[ 1 ] == cCaja } ) ) <> 0
      aData[ n, 2 ]++
   else
      aAdd( aData, { cCaja, 1 } )
   end

RETURN ( Self )































UTILITY STATIC function TTotalTurno_nTotVentas( cCaja, cSerie) ; local Self AS CLASS TTotalTurno := QSelf() AS CLASS TTotalTurno

   local nTotVentas

   nTotVentas  := ::nContadores
   nTotVentas  += ::nTotAlbCliVentas( cCaja, cSerie )
   nTotVentas  += ::nTotFacCliVentas( cCaja, cSerie )
   nTotVentas  += ::nTotRctCliVentas( cCaja, cSerie )
   nTotVentas  += ::nTotTikCliVentas( cCaja, cSerie )
   nTotVentas  += ::nTotAntCliVentas( cCaja, cSerie )
   nTotVentas  += ::nTotChkCliVentas( cCaja, cSerie )
   nTotVentas  -= ::nTotDevCliVentas( cCaja, cSerie )















Return ( nTotVentas )



UTILITY STATIC function TTotalTurno_nTotContadores( cCaja, cSerie) ; local Self AS CLASS TTotalTurno := QSelf() AS CLASS TTotalTurno

   local nTotVentas

   nTotVentas  := ::nContadores

Return ( nTotVentas )



UTILITY STATIC function TTotalTurno_nTotCompras( cCaja, cSerie) ; local Self AS CLASS TTotalTurno := QSelf() AS CLASS TTotalTurno

   local nTotCompras

   nTotCompras := ::nTotAlbPrvCompras( cCaja, cSerie )
   nTotCompras += ::nTotFacPrvCompras( cCaja, cSerie )
   nTotCompras += ::nTotRctPrvCompras( cCaja, cSerie )

Return ( nTotCompras )






UTILITY STATIC function TTotalTurno_nTotCobros( cCaja, cSerie) ; local Self AS CLASS TTotalTurno := QSelf() AS CLASS TTotalTurno

   local nTotCobros

   nTotCobros  := ::nTotContadores( cCaja, cSerie )
   nTotCobros  += ::nTotTikCliCobros( cCaja, cSerie )
   nTotCobros  += ::nTotChkCliCobros( cCaja, cSerie )
   nTotCobros  += ::nTotFacCliCobros( cCaja, cSerie )
   nTotCobros  += ::nTotRctCliCobros( cCaja, cSerie )
   nTotCobros  += ::nTotAntCliVentas( cCaja, cSerie )
   nTotCobros  -= ::nTotTikCliContadores( cCaja, cSerie )
   nTotCobros  -= ::nTotFacCliContadores( cCaja, cSerie )
   nTotCobros  -= ::nTotRctCliContadores( cCaja, cSerie )

Return ( nTotCobros )



UTILITY STATIC function TTotalTurno_nTotCobroSesion( cCaja, cSerie, lIncluirEntregas) ; local Self AS CLASS TTotalTurno := QSelf() AS CLASS TTotalTurno

   local nTotCobro

   IIF( lIncluirEntregas == nil, lIncluirEntregas := .T., ) ;

   nTotCobro                  := ::nTotCobros( cCaja, cSerie )
   nTotCobro                  += ::nTotAntCliVentas( cCaja, cSerie )
   if lIncluirEntregas
      nTotCobro               += ::nTotEntregas( cCaja, cSerie )
   end

Return ( nTotCobro )



UTILITY STATIC function TTotalTurno_nDifCobros( cCaja, cSerie) ; local Self AS CLASS TTotalTurno := QSelf() AS CLASS TTotalTurno

   local nDifCobros

   nDifCobros  := ::nTotVentaContado( cCaja, cSerie )
   nDifCobros  -= ::nTotCobroSesion( cCaja, cSerie, .F. )
   nDifCobros  -= ::nTotEntradas( cCaja, cSerie )

Return ( nDifCobros )



UTILITY STATIC function TTotalTurno_nDifTotal( cCaja, cSerie) ; local Self AS CLASS TTotalTurno := QSelf() AS CLASS TTotalTurno

   local nDifCobros

   nDifCobros  := ::nTotVentaCredito( cCaja, cSerie )

Return ( nDifCobros )



UTILITY STATIC function TTotalTurno_nTotVentaContado( cCaja, cSerie) ; local Self AS CLASS TTotalTurno := QSelf() AS CLASS TTotalTurno

   local nTotVentas

   nTotVentas  := ::nContadores

   nTotVentas  -= ::nTotAlbCliContadores( cCaja, cSerie )
   nTotVentas  += ::nTotTikCliVentas( cCaja, cSerie )
   nTotVentas  += ::nTotFacCliVentas( cCaja, cSerie )
   nTotVentas  += ::nTotRctCliVentas( cCaja, cSerie )
   nTotVentas  += ::nTotEntradas( cCaja, cSerie )
   nTotVentas  += ::nTotAntCliVentas( cCaja, cSerie )
   nTotVentas  += ::nTotChkCliVentas( cCaja, cSerie )
   nTotVentas  -= ::nTotDevCliVentas( cCaja, cSerie )

Return ( nTotVentas )



UTILITY STATIC function TTotalTurno_nTotVentaCredito( cCaja, cSerie) ; local Self AS CLASS TTotalTurno := QSelf() AS CLASS TTotalTurno

   local nTotVenta

   nTotVenta   := ::nTotAlbCliContadores( cCaja, cSerie )
   nTotVenta   += ::nTotAlbCliVentas( cCaja, cSerie )

Return ( nTotVenta )



UTILITY STATIC function TTotalTurno_nTotVentaSesion( cCaja, cSerie) ; local Self AS CLASS TTotalTurno := QSelf() AS CLASS TTotalTurno

   local nTotMetalico

   nTotMetalico      := ::nTotVentas( cCaja, cSerie )
   nTotMetalico      -= ::nTotVentaCredito( cCaja, cSerie )
   nTotMetalico      -= ::nDifCobros( cCaja, cSerie )
   nTotMetalico      += ::nTotEntregas( cCaja, cSerie )
