   end

   ErrorBlock( oBlock )

Return nil



UTILITY STATIC function TMesaTS_ColorEstado( nEstado) ; local Self AS CLASS TMesaTS := QSelf() AS CLASS TMesaTS

   local nColor

   do case
      case ( nEstado = 1 )
         nColor := ::nColorLibre
      case ( nEstado = 2 )
         nColor := ::nColorCurso
      case ( nEstado = 3 )
         nColor := ::nColorOcupado
   end

Return nColor











