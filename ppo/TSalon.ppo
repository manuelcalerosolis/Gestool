   BEGIN SEQUENCE

   ::oSalon          := oSalon

   ::oBtnBmp         := TBtnBmp():New( nTop, nLeft, nWidth, nHeight,,,,, {|| ::SelectedMesa( oWnd ) }, oWnd,,, .F., .T., AllTrim( sPunto:cPuntoVenta ) , oFont,,, !.T., "BOTTOM" )

   ::oBtnBmp:SetColor( nRgb( 0,0,0 ), ::ColorEstado( sPunto:nEstado ) )
