      ::oDbf:nTotPes    += nTotUni * oRetFld( cCodArt, ::oDbfArt, "nPesoKg"  )
      ::oDbf:nTotVol    += nTotUni * oRetFld( cCodArt, ::oDbfArt, "nVolumen" )
