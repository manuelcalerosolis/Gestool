function DrwBar( uCodBar, nTipBar, oGetBar, lCalDigit )

   local oFnt
   local oFntBar
   local cPinta      := ""
   local cFuente     := ""
   local lTexto      := .T.
   local lEan13      := .F.
   local cCodBar

