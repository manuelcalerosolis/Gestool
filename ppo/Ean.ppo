function Ean13Check( cCode )

    local n
   local nControl
   local s1    := 0
   local s2    := 0
   local l     := 10

   for n := 1 TO 6
      s1       := s1 + val( substr( cCode, ( n * 2 ) -1, 1 ) )
      s2       := s2 + val( substr( cCode, ( n * 2 ), 1 ) )
   next

   nControl    := ( s2 * 3 ) + s1

   while nControl > l
      l        := l + 10
   end

   nControl    := l - nControl

return Str( nControl, 1, 0 )
