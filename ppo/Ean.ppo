   IIF( cCodBar == nil, cCodBar := "", ) ;
   IIF( nTipBar == nil, nTipBar := 1, ) ;

   if Valtype( uCodBar ) == "O"
      cCodBar        := uCodBar:VarGet()
   else
      cCodBar        := uCodBar
   end



   if lCalDigit .AND. nTipBar == 1 .AND. len( alltrim( cCodBar ) ) == 12
      cCodBar        := AllTrim( cCodBar ) + Ean13Check( cCodBar )
   end





   lEan13            := ( nTipBar == 1 )                    .AND.  ( (len( AllTrim( cCodBar ) ) <> 13  .AND.  len( AllTrim( cCodBar ) ) <> 15  .AND.  len( AllTrim( cCodBar ) ) <> 18 ).OR.   lBadDigits( AllTrim( cCodBar ) ) )

   if !lEan13

      do case
         case nTipBar == 1
              oFntBar := TFont():New( "UPCHeightA", 5, 40,,,,,,,,,,,,,, )
              cPinta  = If( Empty( cCodBar ) .OR. cCodBar == space( 18 ), "", cEan13( cCodBar ) )
              cFuente = "EAN13"
         case nTipBar == 2
              oFntBar := TFont():New( "Code 39", 1, 70,,,,,,,,,,,,,, )
              cPinta  = If( Empty( cCodBar ) .OR. cCodBar == space( 18 ), "", cCode39( cCodBar ) )
              cFuente = "CODE39"
         case nTipBar == 3
              oFntBar := TFont():New( "Code128B", 7, 60,,,,,,,,,,,,,, )
              cPinta  = If( Empty( cCodBar ) .OR. cCodBar == space( 18 ), "", cCode128( cCodBar ) )
              cFuente = "CODE128"
      endcase



      if Upper( AllTrim( oFntBar:cFaceName ) ) <> "UPCHEIGHTA"   .AND.  Upper( AllTrim( oFntBar:cFaceName ) ) <> "CODE 39"      .AND.  Upper( AllTrim( oFntBar:cFaceName ) ) <> "CODE128B"

         oFnt := TFont():New( "Arial", 0, - 10,,,,,,,,,,,,,, )

         oGetBar:SetFont( oFnt )
