function cEan13( cCode )

   local n
   local cCodeBar := ""
   local nNumero  := 0
   local nAdemdum := 0
   local nNumeroA := 0
   local nNumeroB := 0
   local nNumeroX := 0
   local nValor1  := 8

   cCode          := AllTrim( cCode )
   nValor1        := Val( Left( cCode, 1 )  )

   if Len( cCode ) == 12
      cCode       += Ean13Check( cCode )
   end

   if Len( cCode ) > 13
      if Len( cCode ) == 15
         nAdemdum := 2
      else
         nAdemdum := 5
         nNumeroA := 3 * (  Val( SubStr( cCode, 14, 1 ) ) + Val( SubStr( cCode, 16, 1 ) ) + Val( SubStr( cCode, 18, 1 ) ) )
         nNumeroB := 9 * (  Val( SubStr( cCode, 15, 1 ) ) + Val( SubStr( cCode, 17, 1 ) )                                 )
         nNumeroX := nNumeroA + nNumeroB
         nNumeroX := Val( Right( AllTrim( Str( nNumeroX ) ), 1 )  )
      endif
   endif

   for n := 1 to Len( cCode )
      nNumero = Val( SubStr( cCode, n, 1 ) )
      do case
         case n ==  1
              cCodeBar += Chr( nNumero + 117 )
              cCodeBar += "<"
         case n >=  2 .AND. n <=  7
              cCodeBar += Tabla1( n, nValor1, nNumero )
              if n == 7
                 cCodeBar += "="
              endif
         case n >=  8 .AND. n <= 13
              cCodeBar += Chr( nNumero + 97 )
              if n == 13
                 cCodeBar += "<"
              endif
         case n >= 14 .AND. n <= 15
              if n == 14
                 cCodeBar += " +"
              endif
              if nAdemdum == 2
                 do case
                    case Val( SubStr( cCode ,14, 2  ) ) % 4 == 0
                         cCodeBar += Chr( nNumero +  85 )
                    case Val( SubStr( cCode ,14, 2  ) ) % 4 == 1
                         if n == 14
                            cCodeBar += Chr( nNumero +  85 )
                         else
                            cCodeBar += Chr( nNumero + 128 )
                         endif
                    case Val( SubStr( cCode ,14, 2  ) ) % 4 == 2
                         if n == 14
                            cCodeBar += Chr( nNumero + 128 )
                         else
                            cCodeBar += Chr( nNumero +  85 )
                         endif
                    case Val( SubStr( cCode ,14, 2  ) ) % 4 == 3
                         cCodeBar += Chr( nNumero + 128 )
                 endcase
                if n == 14
                   cCodeBar += "-"
                endif
              else
                 cCodeBar += Tabla5( n, nNumero, nNumeroX )
                 cCodeBar += "-"
              endif
         case n >= 16 .AND. n <= 18
              cCodeBar += Tabla5( n, nNumero, nNumeroX )
              if n <> 18
                 cCodeBar += "-"
              endif
      endcase
   next

return cCodeBar



