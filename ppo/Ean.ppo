function lCalEan13( oCodBar )

   local cCodBar

   if !Empty( oCodBar )

      cCodBar           := oCodBar:VarGet()

      if len( AllTrim( cCodBar ) ) == 12
         cCodBar        := AllTrim( cCodBar ) + Ean13Check( cCodBar )
