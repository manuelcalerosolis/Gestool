            ( dbfProvee )->lSndInt := .F.
            ( dbfProvee )->( dbUnlock() )
         end

         ( dbfProvee )->( dbSkip() )

      end

   RECOVER USING oError

      msgStop( "Imposible abrir todas las bases de datos " + Chr(13)+Chr(10) + ErrorMessage( oError ) )

   end

   ErrorBlock( oBlock )

      ( dbfProvee )->( dbCloseArea() )

   end

