UTILITY STATIC function TProveedorSenderReciver_RestoreData() ; local Self AS CLASS TProveedorSenderReciver := QSelf() AS CLASS TProveedorSenderReciver

   local oBlock
   local oError
   local dbfProvee

   if ::lSuccesfullSend

      oBlock            := ErrorBlock( {| oError | ApoloBreak( oError ) } )
      BEGIN SEQUENCE

      dbUseArea( .T., ( cDriver() ), ( cPatPrv() + "Provee.Dbf" ), ( cCheckArea( "Provee", @dbfProvee ) ), if(.T. .OR. .F., !.F., NIL), .F.,, )
