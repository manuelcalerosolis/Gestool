   if aTmp[ ( dbfProvee )->( fieldpos( "SUBCTA" ) ) ] <> cSubCtaAnt
      LoadSubCta( aTmp[ ( dbfProvee )->( fieldpos( "SUBCTA" ) ) ], cRutCnt(), dbfTmpSubCta )
