      if ( ::oDetCobAge:lFacturaRemesada( lRectificativa, ::oFacCliT:cSerie + Str( ::oFacCliT:nNumFac ) + ::oFacCliT:cSufFac, ::oFacCliT:cCodAge ) ) .AND.  ( ::lFacturasIncluidas .OR. ( !::lFacturasIncluidas .AND. !::oDetCobAge:lFacturaRemesada( lRectificativa, ::oFacCliT:cSerie + Str( ::oFacCliT:nNumFac ) + ::oFacCliT:cSufFac, ::oDbf:cCodAge ) ) )

         lFacturaProcesar  := .T.

      end

   else





      if ( ::lFacturasIncluidas ) .OR. ( !::lFacturasIncluidas .AND. !::oDetCobAge:lFacturaRemesada( lRectificativa, ::oFacCliT:cSerie + Str( ::oFacCliT:nNumFac ) + ::oFacCliT:cSufFac, ::oFacCliT:cCodAge ) )

