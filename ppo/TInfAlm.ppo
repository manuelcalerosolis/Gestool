UTILITY STATIC function TInfAlm_IncluyeCero() ; local Self AS CLASS TInfAlm := QSelf() AS CLASS TInfAlm





   ::oDbfAlm:GoTop()

   while !::oDbfAlm:Eof()


      if ( ::lAllAlm .OR. ( ::oDbfAlm:cCodAlm >= ::cAlmOrg .AND. ::oDbfAlm:cCodAlm <= ::cAlmDes ) ) .AND. !::oDbf:Seek( ::oDbfAlm:cCodAlm )

         ::oDbf:Append()
