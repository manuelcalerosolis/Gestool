UTILITY STATIC function TInfAlm_AcuTikCli( cCodArt, nPrecio) ; local Self AS CLASS TInfAlm := QSelf() AS CLASS TInfAlm

   if ::oDbf:Seek( ::oTikCliT:cAlmTik )



      ::oDbf:Load()

      if ::oTikCliT:cTipTik == "4"
        ::oDbf:nNumUni  -= ::oTikCliL:nUntTil
      else
        ::oDbf:nNumUni  += ::oTikCliL:nUntTil
      end

      ::oDbf:nImpArt    += nImpUTpv( ::oTikCliT:cAlias, ::oTikCliL:cAlias, ::nDecOut, ::nValDiv, nil, nPrecio )
      ::oDbf:nImpTot    += nImpLTpv( ::oTikCliT:cAlias, ::oTikCliL:cAlias, ::nDecOut, ::nDerOut, ::nValDiv, nil, nPrecio )
      ::oDbf:nIvaTot    += nIvaLTpv( ::oTikCliT:cAlias, ::oTikCliL:cAlias, ::nDecOut, ::nDerOut, ::nValDiv, nPrecio )
      ::oDbf:nTotFin    += nImpLTpv( ::oTikCliT:cAlias, ::oTikCliL:cAlias, ::nDecOut, ::nDerOut, ::nValDiv, nil, nPrecio )
      ::oDbf:nTotFin    += nIvaLTpv( ::oTikCliT:cAlias, ::oTikCliL:cAlias, ::nDecOut, ::nDerOut, ::nValDiv, nPrecio )
      ::oDbf:nPreMed    := ::oDbf:nImpTot / ::oDbf:nNumUni

      ::AcuPesVol( cCodArt, ::oTikCliL:nUntTil, ::oDbf:nImpTot, .T. )

      ::oDbf:Save()

   else



      ::AppTikCli( cCodArt, nPrecio, .T. )

   end

RETURN ( self )



UTILITY STATIC function TInfAlm_AddRAlb() ; local Self AS CLASS TInfAlm := QSelf() AS CLASS TInfAlm

