FUNCTION fnDBStruct()
LOCAL Result := "", Arr, x, y
   IF frReportManager():bDbStruct == NIL
     Arr := DbStruct()
   ELSE
     Arr := Eval(frReportManager():bDbStruct)
   ENDIF
   for x = 1 to Len(Arr)
      for y = 1 to Len(Arr[x])
         DO CASE
            CASE (y = 1)
              Result := Result + Arr[x,y]
            CASE (y = 2)
              Result := Result + "," + Arr[x,y]
            CASE (y = 3).OR.(y = 4)
              Result := Result + "," + Alltrim(Str(Arr[x,y], 10, 0))
         ENDCASE
      next
      Result := Result + ";"
   next
RETURN Result
