      if ( ::lAllFpg .OR. ( ::oDbfFpg:cCodPago >= ::cFpgDes .AND. ::oDbfFpg:cCodPago <= ::cFpgHas ) ) .AND. !::oDbf:Seek( ::oDbfFpg:cCodPago )

         ::oDbf:Append()
